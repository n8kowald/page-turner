{
  "name": "Google+ Fixed Circlebar",
  "version": "1.0",
  "description": "Keeps the circlebar fixed in Google+",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["http://plus.google.com/*", "https://plus.google.com/*"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "http://*/*"
  ]
}
