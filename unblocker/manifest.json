{
  "name": "Unblocker", 
  "version": "1.0",
  "description": "Attempts to load blocked websense content",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon64.png" },
  "background_page": "background.html",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "functions.js"]
    }
  ]
}
