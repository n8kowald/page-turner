{
  "name": "KB Shortcut Sniffer",
  "version": "1.0",
  "description": "Looks for keyboard shortcut mappings and accesskeys, if found tells you",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "functions.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "http://*/*"
  ]
}
